# Korrigierte Caddy Konfiguration mit richtigen Container-Namen

# Bestehende Services (migriert von Apache2)
affine.ecospherenet.work {
    reverse_proxy affine_server:3010
    tls admin@ecospherenet.work
}

appflowy.ecospherenet.work {
    reverse_proxy appflowy-cloud-nginx-1:80
    tls admin@ecospherenet.work
}

gitlab.ecospherenet.work {
    reverse_proxy gitlab-gitlab:80
    tls admin@ecospherenet.work
}

openproject.ecospherenet.work {
    reverse_proxy localhost:8080
    tls admin@ecospherenet.work
}

