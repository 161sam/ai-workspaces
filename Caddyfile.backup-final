# Globale Konfiguration (MUSS ZUERST STEHEN!)
{
    email {$ACME_EMAIL}
    admin off
}

# n8n Service
{$N8N_HOSTNAME} {
    reverse_proxy n8n:5678
}

# Desktop/Guacamole Service
{$GUACAMOLE_HOSTNAME} {
    reverse_proxy guacamole-web:8080
}

# Flowise Service
{$FLOWISE_HOSTNAME} {
    reverse_proxy flowise:3000
}

# Portainer Service
{$PORTAINER_HOSTNAME} {
    reverse_proxy portainer:9000
}
